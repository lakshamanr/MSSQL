{
  "name": "mssql",
  "version": "0.0.0",
  "main": "electron/main.js",
  "scripts": {
    "electron": "ng build --prod && electron ./electron/main.js && npm run copy-electron",
    "copy-electron": "xcopy /E /I /Y node_modules\\electron\\dist Electron",
    "electron:build": "ng build --prod && electron-builder",
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
    "package": "electron-packager . mssql --platform=win32 --arch=x64 --out=release-builds --overwrite --asar --icon=electron/icon.ico"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "8.2.13",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "8.2.13",
    "@angular/forms": "8.2.13",
    "@angular/http": "6.1.10",
    "@angular/platform-browser": "8.2.13",
    "@angular/platform-browser-dynamic": "8.2.13",
    "@angular/platform-server": "8.2.13",
    "@angular/router": "8.2.13",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "@nguniversal/module-map-ngfactory-loader": "6.0.0",
    "@ngx-prism/core": "^2.0.1",
    "@types/prismjs": "^1.16.0",
    "amexio-ng-extensions": "^5.20.4",
    "angular-font-awesome": "^3.1.2",
    "angular-progress-bar": "^1.0.11",
    "angular-split": "^3.0.3",
    "angular-svg-pan-zoom": "^1.0.0",
    "angular2-multiselect-dropdown": "^4.6.3",
    "aspnet-prerendering": "^3.0.1",
    "bootstrap": "^4.4.1",
    "core-js": "^2.6.11",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "html-query-plan": "^2.6.0",
