    "ngx-filesaver": "^10.0.0",
    "ngx-prism": "0.0.1",
    "ngx-ui-loader": "^8.0.0",
    "popper.js": "^1.16.1",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.0",
    "prismjs": "^1.30.0",
    "rxjs": "^6.5.4",
    "svg-pan-zoom": "^3.6.1",
    "zone.js": "^0.9.1"
